@model  List<openCaseMaster.Models.openTestStep>


        <table class="table" style="table-layout:fixed">

            <thead>
                <tr>
                    <th width="15%">步骤</th>
                    <th>描述</th>
                    <th style="width:40%;">样例图</th>
                </tr>
            </thead>
            <tbody>
                @{int i = 0;}
                @foreach (var s in Model)
                {
                    i++;
                    <tr>
                        <td>@i</td>
                        <td>@s.describe</td>
                        <td><img class="carousel-inner img-responsive img-rounded demoImg" src="@s.demoImg" /></td>
                    </tr>

                }

            </tbody>
        </table>


