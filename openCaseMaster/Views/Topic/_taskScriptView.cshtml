@model openCaseMaster.ViewModels.taskModel

<div id="reply" class="panel panel-default">
      <div class="panel-heading"><b>测试app:</b><a href="#">@Model.appName</a></div>
      <div class="panel-body">

       <table class="table" style="table-layout:fixed">
        
        <thead>
            <tr>
                <th width="20%">编号</th>
                <th>案例</th>
            </tr>
        </thead>
        <tbody>
            @{int i = 1;}
            @foreach (var d in Model.taskScripts)
            {
                <tr>
                    <td>@i</td>
                    <td nowrap style="overflow:hidden; text-overflow:ellipsis;"><a href="#">@d.Value</a></td>
                </tr>
                { i++; }
            }

        </tbody>
    </table>

          </div>

    </div>

    


